import java.util.Scanner;
public class JPA02{
    public static void main(String[] args){
        while(true){
            System.out.println("Input:");
            Scanner a = new Scanner(System.in);
            int b = a.nextInt();
            int c = a.nextInt();
            if (b>c){
                System.out.println(b+" is larger than "+c);
            }else{
                System.out.println(c+" is larger than "+b);
            }
        }
    }
}
