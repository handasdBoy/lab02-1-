import java.util.Scanner;
public class JPA04{
    public static void main(String[] args){
        while(true){
            System.out.println("Input:");
            Scanner a = new Scanner(System.in);
            int b = a.nextInt();
            if (b%5==0 && b%9==0){
                System.out.println("Yes");
            }else{
                System.out.println("No");
            }
        }
    }
}
